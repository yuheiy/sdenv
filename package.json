{
  "type": "module",
  "scripts": {
    "dev": "concurrently --raw \"npm:dev:*\"",
    "dev:astro": "astro dev",
    "dev:css": "chokidar \"src/styles/{base,components,utilities}.*.css\" --command \"touch tailwind.config.cjs\" --debounce 0 --silent",
    "build": "astro check && tsc --noEmit && astro build",
    "preview": "astro preview",
    "test": "vitest",
    "lint": "prettier . --check && eslint .",
    "lint:fix": "prettier . --write && eslint . --fix",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@alpinejs/collapse": "3.13.2",
    "@alpinejs/focus": "3.13.2",
    "@alpinejs/ui": "3.13.2-beta.1",
    "@astrojs/check": "0.3.1",
    "@astrojs/mdx": "1.1.5",
    "@astrojs/sitemap": "3.0.3",
    "@astrojs/tailwind": "5.0.2",
    "@tailwindcss/container-queries": "0.1.1",
    "@types/alpinejs": "3.13.5",
    "alpinejs": "3.13.2",
    "astro": "3.5.5",
    "patch-package": "8.0.0",
    "rollup-plugin-visualizer": "5.9.2",
    "tailwindcss-animate": "1.0.7",
    "tiny-invariant": "1.3.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "chokidar-cli": "3.0.0",
    "concurrently": "8.2.2",
    "eslint": "8.53.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-astro": "0.29.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-tailwindcss": "3.13.0",
    "jsdom": "22.1.0",
    "prettier": "3.1.0",
    "prettier-plugin-astro": "0.12.2",
    "prettier-plugin-css-order": "2.0.1",
    "prettier-plugin-tailwindcss": "0.5.7",
    "vitest": "0.34.6"
  },
  "engines": {
    "node": "20"
  },
  "packageManager": "npm@10.1.0",
  "private": true
}
