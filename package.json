{
  "name": "my-website",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "astro build",
    "dev": "astro dev",
    "fix": "pnpm run fix:eslint && pnpm run fix:prettier",
    "fix:eslint": "pnpm run lint:eslint --fix",
    "fix:prettier": "pnpm run lint:prettier --write",
    "lint": "astro sync && pnpm run \"/^lint:.*/\"",
    "lint:astro": "astro check",
    "lint:eslint": "eslint --max-warnings 0",
    "lint:prettier": "prettier --check .",
    "lint:type-check": "tsc -b",
    "preview": "astro preview",
    "test": "vitest --passWithNoTests"
  },
  "dependencies": {
    "@astrojs/sitemap": "3.3.0",
    "@tailwindcss/vite": "4.1.0",
    "@yuheiy/postcss-custom-functions": "0.1.1",
    "alpinejs": "3.14.9",
    "astro": "5.5.6",
    "astro-icon": "1.1.5",
    "sharp": "0.33.5",
    "tailwindcss": "4.1.0",
    "tiny-invariant": "1.3.3"
  },
  "devDependencies": {
    "@astrojs/check": "0.9.4",
    "@eslint/js": "9.23.0",
    "@types/alpinejs": "3.13.11",
    "eslint": "9.23.0",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "prettier": "3.5.3",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-css-order": "2.1.2",
    "prettier-plugin-organize-imports": "4.1.0",
    "prettier-plugin-tailwindcss": "0.6.11",
    "typescript": "5.8.2",
    "typescript-eslint": "8.29.0",
    "vitest": "3.1.1"
  },
  "packageManager": "pnpm@10.7.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp"
    ]
  }
}
