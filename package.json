{
  "private": true,
  "type": "module",
  "scripts": {
    "build": "astro check && tsc && astro build",
    "dev": "concurrently --raw \"npm:dev:*\"",
    "dev:astro": "astro dev --host",
    "dev:css": "chokidar \"src/styles/**/*.css\" --command \"touch tailwind.config.cjs\" --debounce 0 --silent",
    "fix": "npm run fix:prettier && npm run fix:eslint",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "lint": "concurrently --raw \"npm:lint:*\"",
    "lint:eslint": "eslint . --max-warnings 0",
    "lint:prettier": "prettier . --check",
    "preview": "astro preview",
    "test": "vitest"
  },
  "dependencies": {
    "@alpinejs/collapse": "3.13.7",
    "@alpinejs/focus": "3.13.7",
    "@alpinejs/ui": "3.13.7-beta.0",
    "@astrojs/check": "0.5.7",
    "@astrojs/mdx": "2.2.0",
    "@astrojs/sitemap": "3.1.1",
    "@astrojs/tailwind": "5.1.0",
    "@eslint/eslintrc": "3.0.2",
    "@tailwindcss/container-queries": "0.1.1",
    "@types/alpinejs": "3.13.7",
    "@typescript-eslint/parser": "7.3.1",
    "alpinejs": "3.13.7",
    "astro": "4.5.1",
    "astro-icon": "1.1.0",
    "chokidar-cli": "3.0.0",
    "concurrently": "8.2.2",
    "eslint": "8.57.0",
    "eslint-plugin-astro": "0.32.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-tailwindcss": "3.15.0",
    "prettier": "3.2.5",
    "prettier-plugin-astro": "0.13.0",
    "prettier-plugin-css-order": "2.0.1",
    "prettier-plugin-tailwindcss": "0.5.12",
    "rollup-plugin-visualizer": "5.12.0",
    "tailwindcss-animate": "1.0.7",
    "tiny-invariant": "1.3.3",
    "typescript-eslint": "7.3.1",
    "vitest": "1.3.1"
  },
  "packageManager": "npm@10.5.0",
  "engines": {
    "node": "20",
    "npm": "10"
  }
}
